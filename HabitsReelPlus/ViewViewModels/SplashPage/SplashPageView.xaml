<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HabitsReelPlus.ViewViewModels.SplashPage.SplashPageView"
             Title="{Binding Title}">
    <VerticalStackLayout>
        <Button Grid.Row="1"
                    Grid.ColumnSpan="2"
                    Text="{Binding SettingTitle}"
                    Command="{Binding OnSettingClicked}" />
        <Button Grid.Row="2"
                    Grid.ColumnSpan="2"
                    Text="{Binding WorkoutTitle}"
                    Command="{Binding OnWorkoutClicked}" />
    </VerticalStackLayout>
</ContentPage>